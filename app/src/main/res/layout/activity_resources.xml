<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".Resources">

    <Button
        android:id="@+id/youtube"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_marginEnd="9dp"
        android:layout_marginTop="36dp"
        android:text="Go"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/khanAcademy"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_marginEnd="9dp"
        android:layout_marginTop="40dp"
        android:text="Go"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.944"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/wolfram"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_marginEnd="9dp"
        android:layout_marginTop="56dp"
        android:text="Go"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="90dp"
        android:layout_height="95dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/youtube" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="89dp"
        android:layout_height="101dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:srcCompat="@drawable/khan_academy" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="91dp"
        android:layout_height="113dp"
        android:layout_marginEnd="14dp"
        android:layout_marginStart="16dp"
        app:layout_constraintEnd_toStartOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:srcCompat="@drawable/wolfram" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="152dp"
        android:layout_height="66dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="40dp"
        android:text="Use this computational knowledge engine to help your with your studies."
        android:textColor="@android:color/black"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="159dp"
        android:layout_height="63dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="40dp"
        android:text="Find online course and tools that can help you with your studies."
        android:textColor="@android:color/black"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="159dp"
        android:layout_height="63dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="24dp"
        android:text="Find video tutorials for your subject to help you out with your studies."
        android:textColor="@android:color/black"
        app:layout_constraintEnd_toStartOf="@+id/youtube"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="95dp"
        android:layout_height="96dp"
        android:layout_marginEnd="13dp"
        android:layout_marginStart="16dp"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:srcCompat="@drawable/quizlet" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="161dp"
        android:layout_height="84dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="33dp"
        android:text="Use this website to create quizzes, flashcards and games for your subject to make studying easier."
        android:textColor="@android:color/black"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/quizlet"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_marginEnd="9dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="59dp"
        android:text="Go"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/wolfram" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="100dp"
        android:layout_height="106dp"
        android:layout_marginEnd="13dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toStartOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:srcCompat="@drawable/sparknotes" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="157dp"
        android:layout_height="58dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="45dp"
        android:text="Find guides and tests for your chosen subject to help your studies."
        android:textColor="@android:color/black"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <Button
        android:id="@+id/sparknotes"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_marginEnd="24dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="80dp"
        android:text="Go"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/quizlet" />

</android.support.constraint.ConstraintLayout>